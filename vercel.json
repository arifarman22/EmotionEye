{
  "rewrites": [
    { "source": "/", "destination": "/Frontend/standalone.html" },
    { "source": "/(.*)", "destination": "/Frontend/standalone.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}